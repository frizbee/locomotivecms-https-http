{
  "manifest_version": 2,

  "name": "LocomotiveCMS https to http",
  "description": "Replace sign in url from https to http for development",
  "version": "0.2.0",

  "icons": {
    "16": "img/icon16.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },

  "browser_action": {
   "default_icon": "img/icon.png",
   "default_popup": "popup.html",
   "default_title": "Settings"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
     {
       "matches": ["*://*/*"],
       "css": ["css/styles.css"],
       "js": ["js/app.js"]
     }
   ]
}
